{
    // LOGO
    "custom/appmenu": {
        "format": "   ",
        "on-click": "sleep 0.2;~/.config/rofi/launcher.sh",
        "tooltip-format": "Open the application launcher"
    },
    // WORKSPACES
    "niri/workspaces": {
        // "format": "{name}",
        "on-click": "activate"
    },
    // TIMES
    "clock": {
        "format": "{:%I:%M %p}",
        "tooltip": false
    },
    // NETWORK
    "network": {
        "interface": "ens192",
        "format": "{ifname}",
        // "format-wifi": "Ôá´ ",
        "format-wifi": "{icon}",
        "format-icons": [
            "Û∞§†",
            "Û∞§ü",
            "Û∞§¢",
            "Û∞§¢",
            "Û∞§®"
        ],
        "format-ethernet": "Û∞àÄ",
        "format-disconnected": "Û∞§≠", //An empty format will hide the module.
        "tooltip-format": "{ifname} via {gwaddr} Û∞àÄ ",
        "tooltip-format-wifi": "{essid} ({signalStrength}%) Ôá´ ",
        "tooltip-format-ethernet": "{ifname} üñß ",
        "tooltip-format-disconnected": "Disconnected",
        "max-length": 50
    },
    // AUDIO
    "pulseaudio": {
        "format": "{volume}% {icon}",
        "format-bluetooth": "{icon}Ôäî",
        "format-muted": "Óª®",
        "format-icons": {
            "alsa_output.pci-0000_00_1f.3.analog-stereo": "Óå≠",
            "alsa_output.pci-0000_00_1f.3.analog-stereo-muted": "Óª®Óå≠",
            "headphone": "ÔÄ•",
            "hands-free": "Ôñê",
            "headset": "Ôñê",
            "phone": "ÔÇï",
            "phone-muted": "Ó¥ó",
            "portable": "ÔÇï",
            "car": "ÔÜπ",
            "default": [
                "ÔÄß",
                "ÔÄ® "
            ]
        },
        "scroll-step": 1,
        "on-click": "pavucontrol",
        "ignored-sinks": [
            "Easy Effects Sink"
        ]
    },
    // BATTERY
    "battery": {
        "interval": 30,
        "states": {
            "good": 95,
            "warning": 30,
            "critical": 20
        },
        "format": "{capacity}% {icon}",
        "format-charging": "{capacity}% Û∞ÇÑ",
        "format-plugged": "{capacity}% Û∞ÇÑ Ó¨≠",
        "format-alt": "{time} {icon}",
        "format-icons": [
            "Û∞Åª",
            "Û∞Åº",
            "Û∞Åæ",
            "Û∞ÇÄ",
            "Û∞ÇÇ",
            "Û∞Åπ"
        ]
    },
    // SWAYNC
    "custom/notification": {
        "tooltip": true,
        "format": "{icon}",
        "format-icons": {
            "notification": "Û±Ö´",
            "none": "Û∞Çú",
            "dnd-notification": "Û∞Ç†",
            "dnd-none": "Û∞™ì",
            "inhibited-notification": "Û∞Çõ",
            "inhibited-none": "Û∞™ë",
            "dnd-inhibited-notification": "Û∞Çõ",
            "dnd-inhibited-none": "Û∞™ë"
        },
        "return-type": "json",
        "exec-if": "which swaync-client",
        "exec": "swaync-client -swb",
        "on-click": "swaync-client -t -sw",
        "on-click-right": "swaync-client -d -sw",
        "escape": true
    },
    // POWER-PROFILES-DAEMON
    "power-profiles-daemon": {
        "format": "{icon}",
        "tooltip-format": "Power profile: {profile}\nDriver: {driver}",
        "tooltip": true,
        "format-icons": {
            "default": "ÔÉß",
            "performance": "ÔÉß",
            "balanced": "Û∞óë",
            "power-saver": "ÔÅ¨"
        }
    },
    // POWER BUTTON
    "custom/power": {
        "format": "Û∞§Ü",
        "on-click": "wlogout"
    },
    // TRAY
    "tray": {
        "icon-size": 14,
        "spacing": 10
    },
    "bluetooth": {
        "format": "{status} Ôäî",
        "format-disabled": "Û∞Ç≤",
        "format-no-controller": "",
        "tooltip-format-no-controller": "",
        "tooltip-format": "{controller_alias} {controller_address}\n{num_connections} connected",
        "tooltip-format-connected": "{controller_alias} {controller_address}\n{num_connections} connected\n{device_enumerate}",
        "tooltip-format-enumerate-connected": "{device_alias} {device_address}",
        "tooltip-format-enumerate-connected-battery": "{device_alias} {device_address} {device_battery_percentage}%"
    },
    // ===========================================================
    // OTHER
    "custom/expand": {
        "format": "ÔÑÑ",
        "tooltip": false
    },
    "custom/end": {
        "format": "|",
        "tooltip": false
    },
    "group/expand": {
        "orientation": "horizontal",
        "drawer": {
            "transition-duration": 600,
            "transition-to-left": true,
            "click-to-reveal": true
        },
        "modules": [
            "custom/expand",
            "pulseaudio",
            "bluetooth",
            "custom/end"
        ]
    }
}
